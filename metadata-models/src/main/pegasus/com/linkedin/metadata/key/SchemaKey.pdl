namespace com.linkedin.metadata.key

/**
 * Key for a Schema
 */
@Aspect = {
  "name": "schemaKey"
}
record SchemaKey {
  /**
  * A unique id for the Schema.
  * There are no rules for what constitutes a valid id for a Schema. 
  * Valid options are:
  * - guid("schema-registry", subject-name, version)
  * - guid("custom-platform-name", namespace, name, version)
  */
  id: string
}
